  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/expo": "^33.0.1",
    "@types/jest": "^26.0.15",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
